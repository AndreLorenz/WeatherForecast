{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "rules": {
        "no-underscore-dangle": 0,
        "no-console": 0,
        "comma-dangle": 0,
        "import/prefer-default-export": 0,
        "global-require": 0,
        "arrow-parens": 0,
        "curly": 0,
        "class-methods-use-this": 0,
        "max-len": [
            "error", 180
        ]
    }
}